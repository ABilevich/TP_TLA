num centery
num centerx
num width
num height
num verticalDistance
num horizontalDistance
num planetQuantity
num i

main(){
    Config.worldBorderBounce(true)
    Config.gravityConstant(0)

    width = getWidth()
    height = getHeigth()

    planetQuantity = readNum("Insert planet quantity (Recomended 30-50):")

    verticalDistance = floor(height / planetQuantity)
    horizontalDistance = floor(width / planetQuantity)

    i = 0
    while(i < planetQuantity){
        System.addBody( 50,  50+verticalDistance*i,    10000     , 5, 10  ,0      , "#cf5219")
        i+=1
    }
    i = 0

    while(i < planetQuantity){
        System.addBody( 20 + horizontalDistance*i, 20 ,    10000     , 5,  0 ,10      , "#ff0019")
        i+=1
    }

}